<div class="page-feedback-block" data-entity-type="{{ entity_type }}" data-entity-id="{{ entity_id }}">
  <div class="page-feedback-header">
    <div class="page-feedback-question">
      {{ question }}
    </div>
    <div class="page-feedback-buttons">
      <button type="button" class="page-feedback-btn page-feedback-yes">
        {{ 'Yes'|t }}
      </button>
      <button type="button" class="page-feedback-btn page-feedback-no">
        {{ 'No'|t }}
      </button>
    </div>
  </div>
  <div class="page-feedback-summary">
    <span class="page-feedback-text">
      {{ 'No feedback yet.'|t }}

    </span>
  </div>
</div>